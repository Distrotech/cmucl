# -*- Mode: makefile -*-

include Config.x86_common

# Compile code that will run on OSX 10.4 (Tiger)
MIN_VER = -mmacosx-version-min=10.4

CPPFLAGS += -DDARWIN $(MIN_VER)
CFLAGS += -g3
ASFLAGS += -g3 $(MIN_VER)


UNDEFSYMPATTERN = -Xlinker -u -Xlinker &

OS_SRC += Darwin-os.c 
OS_LINK_FLAGS = $(MIN_VER)
OS_LIBS =

